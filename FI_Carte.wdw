   -
     name : AfficheAdresse
     procedure_id : 1629807829841456930
     type_code : 14
     code : |1+
      // Résumé : Affiche l'adresse dans la carte
      // Syntaxe :
      // AfficheAdresse ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      
      PROCEDURE AfficheAdresse(sAdresse est une chaîne)
      
      //Marqueur de position dans la carte
      Localisation est un gglCoordonnée
      Localisation = GglAdresseVersCoordonnées(CST_CLÉGOOGLE, sAdresse)
      
      si erreurdetectée = vrai ALOrS
      	//On tente de localiser juste la commune
      	Localisation = GglAdresseVersCoordonnées(CST_CLÉGOOGLE, extraitchaine(sAdresse, 3, RC, DepuisFin))
      	si erreurdetectée = vrai ALOrS
      		retour
      	Fin
      FIN
      
      MarqueurLoc est un Marqueur
      MarqueurLoc.Position..Latitude = Localisation..Latitude
      MarqueurLoc.Position..Longitude = Localisation..Longitude
      
      
      CARTE_Localisation..ModeCarte = carteModeSatellite
      CARTE_Localisation..Zoom = 16
      
      CarteAffichePosition(CARTE_Localisation,MarqueurLoc.Position)
      CarteAjouteMarqueur(CARTE_Localisation, MarqueurLoc)
     type : 458752
