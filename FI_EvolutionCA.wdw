     name : RemplissageGraphe
     procedure_id : 1628369767230219451
     type_code : 14
     code : |1-
      // Résumé : Remplissage du graphe avec le CA des 12 derniers mois (et le comparatif avec l'année N-1)
      // Syntaxe :
      //RemplissageGraphe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE RemplissageGraphe()
      
      dDate est une date 
      
      pour i = 1 a 12
      	
      	REQ_CAMois.paramMois = gauche(dDate,6)
      	HExécuteRequête(REQ_CAMois)
      	hlitpremier(REQ_CAMois)	
      	grAjouteDonnée(GRF_EvolutionCA, 1, 12-i, REQ_CAMois.la_somme_TotalTTC)
      	
      	//Et l'année précédente
      	dDate..année--
      	REQ_CAMois.paramMois = Gauche(dDate,6)
      	HExécuteRequête(REQ_CAMois)
      	HLitPremier(REQ_CAMois)	
      	grAjouteDonnée(GRF_EvolutionCA, 2, 12-i, REQ_CAMois.la_somme_TotalTTC)
      		
      	grEtiquetteCatégorie(GRF_EvolutionCA, 12-i, DateVersChaîne(dDate,"Mmm"))
      	
      	dDate..Année++
      	dDate..mois --
      	
      fin
      
      grdessine(GRF_EvolutionCA)
     type : 458752
