            si htrouve(DéclinaisonProduit) = faux ALORS
            	Erreur("Vous devez saisir une référence valide.")
            	reprisesaisie(SAI_Ajout_Référence)
            FIN
            
            // Recherche le produit lié à la déclinaison
            HLitRecherchePremier(produit, IDProduit, DéclinaisonProduit.IDProduit)
            
            nIndice est un entier
            // Si le produit est déjà dans la table
            nIndice = tablecherche(TABLE_LigneCde.COL_Reference, SAI_Ajout_Référence)
            si nIndice > 0 ALORS
            	
            	// Demande à l'utilisateur ce qu'il souhaite faire (remplacer la quantité, incrémenter la quantité, ...)
            	selon dialogue(chaineconstruit("Le produit #%1 - %2 est déjà présent dans les lignes de la commande.", SAI_Ajout_Référence, SAI_Ajout_Libellé) +rc+ "Que souhaitez-vous faire ?", ["Remplacer la quantité", "Incrémenter la quantité", "Annuler"], 1, 3)
            		// Remplacer la quantité
            		CAS 1
            			TABLE_LigneCde.COL_Quantite[nIndice] = SAI_Ajout_Quantité
            			
            		// Incrémenter la quantité
            		CAS 2
            			TABLE_LigneCde.COL_Quantite[nIndice] += SAI_Ajout_Quantité
            			
            		// Annuler
            		CAS 3
            			// Vide les champs
            			SAI_Ajout_Référence = ""
            			SAI_Ajout_Libellé = ""
            			RETOUR
            			
            	FIN	
            	
            SINON // Le produit n'est pas encore dans la table
            	
            	// Ajoute une ligne dans la table
            	nIndice = tableajouteligne(TABLE_LigneCde, 0, 0, SAI_Ajout_Référence)
            	COL_LibProd[nIndice] = DéclinaisonProduit.LibelleDeclinaison
            	COL_PrixUnitaireHT[nIndice] = DéclinaisonProduit.PrixUnitaireHT
            	COL_Quantite[nIndice] = SAI_Ajout_Quantité
            	
            	REQ_TauxTaxeDéclinaisonProduit.ParamIDDéclinaisonProduit = DéclinaisonProduit.IDDéclinaisonProduit
            	HExécuteRequête(REQ_TauxTaxeDéclinaisonProduit)
            	HLitPremier(REQ_TauxTaxeDéclinaisonProduit)
            	COL_TauxTVA[nIndice] = REQ_TauxTaxeDéclinaisonProduit.Taux
            	
            FIN
            
            // Actualise les totaux
            COL_SousTotalHT[nIndice] = COL_Quantite[nIndice] * COL_PrixUnitaireHT[nIndice]
            CalculeTotaux()
            
            // Vide les champs
            SAI_Ajout_Libellé = ""
            SAI_Ajout_Référence = ""
            SAI_Ajout_Quantité = 0
