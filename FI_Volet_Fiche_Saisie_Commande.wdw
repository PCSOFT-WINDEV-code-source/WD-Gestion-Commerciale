    type : 32
    code_elements :
     type_code : 70
     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre(gnIDCommande est un entier = -1, gnStatut est un entier = CDE_ATTENTEREGLEMENT, gUUIDClient est un UUID = Nil_UUID)
         
         gbChoixAssisté est une booleen
      -
        code : |1+
         
         //Si on est en modification d'une commande / d'un devis existant, on l'affiche
         si gnIDCommande <> -1 alors 
         	
         	HLitRecherchePremier(Commande,IDCommande,gnIDCommande)
         	si htrouve(commande) alors
         		
         		//Affichage des informations de commande
         		FichierVersEcran()
         		
         		LIB_Total = commande.TotalàPayer
         		
         		//Le statut de la commande : devis / commande en attente de paiement / en cours de préparation..
         		gnStatut = commande.EtatCommande
         		
         		//Le client
         		gUUIDClient = Commande.UUIDClient	
         		SAI_Client = ClientNomComplet(gUUIDClient)
         		
         		//Les lignes de commande
         		REQ_LignesCdeDuneCde.ParamIDCommande = gnIDCommande
         		HExécuteRequête(REQ_LignesCdeDuneCde)
         		pour TOUT REQ_LignesCdeDuneCde 
         			TableAjouteLigne(TABLE_LigneCde, REQ_LignesCdeDuneCde.IDLigneCde,gnIDCommande, REQ_LignesCdeDuneCde.Reference, REQ_LignesCdeDuneCde.LibelleProduit,  REQ_LignesCdeDuneCde.PrixUnitaireHT,  REQ_LignesCdeDuneCde.Quantite, REQ_LignesCdeDuneCde.PrixUnitaireHT * REQ_LignesCdeDuneCde.Quantite,REQ_LignesCdeDuneCde.TauxTaxe) 
         		FIN
         		
         		CalculeTotaux()
         		
         		//Les adresses de livraison et de facturation
         		
         		HLitRecherchePremier(AdresseArchive,IDAdresse,commande.IDAdresseLivraison)
         		SI HTrouve(AdresseArchive) ALORS
         			
         			SAI_AdresseNomComplet = AdresseArchive.NomComplet
         			SAI_AdresseSociété = AdresseArchive.Societe
         			SAI_AdresseVoie = AdresseArchive.Voie
         			SAI_AdresseComplément = AdresseArchive.Complement
         			SAI_AdresseBoîtePostale = AdresseArchive.LieuDitBP
         			SAI_AdresseCodePostal = AdresseArchive.CodePostal
         			SAI_AdresseVille = AdresseArchive.Ville
         			SAI_AdresseEtatProvince = AdresseArchive.EtatProvince
         			nIndicePays est un entier = ListeCherche(COMBO_AdressePays,AdresseArchive.Pays)
         			SI nIndicePays > 0 ALORS ListeSelectPlus(COMBO_AdressePays, nIndicePays)
         		fin
         		
         		
         		HLitRecherchePremier(AdresseArchive,IDAdresse,Commande.IDAdresseFacturation)
         		SI HTrouve(AdresseArchive) ALORS
         			//Adresse de facturation, pré-remplie avec l'adresse du client
         			SAI_AdresseFactNomComplet = AdresseArchive.NomComplet
         			SAI_AdresseFactSociété = AdresseArchive.Societe
         			SAI_AdresseFactVoie = AdresseArchive.Voie
         			SAI_AdresseFactComplément = AdresseArchive.Complement
         			SAI_AdresseFactBoîtePostale = AdresseArchive.LieuDitBP
         			SAI_AdresseFactCodePostal = AdresseArchive.CodePostal
         			SAI_AdresseFactVille = AdresseArchive.Ville
         			SAI_AdresseFactEtatProvince = AdresseArchive.EtatProvince
         			nIndicePays est un entier = ListeCherche(COMBO_AdresseFactPays,AdresseArchive.Pays)
         			SI nIndicePays > 0 ALORS ListeSelectPlus(COMBO_AdresseFactPays, nIndicePays)
         		fin
         		
         		
         		SI Commande.IDAdresseFacturation = Commande.IDAdresseLivraison ALORS
         			
         			INT_AdresseFacturationIdentique = 1
         			GR_AdresseFacturation..Etat = Grisé
         			
         		FIN
         		
         		
         	sinon
         		erreur("Commande non retrouvée")
         		retour
         	fin
         	
         	
         sinon
         	
         	//Sinon on va essayer de pré-remplir un maximum d'informations, notamment concernant le client, si on le connaît 
         	SI gUUIDClient <> Nil_UUID ALORS
         		
         		//On pré-remplit le nom du client	
         		SAI_Client = ClientNomComplet(gUUIDClient)
         		
         		//Adresse de livraison, pré-remplie avec l'adresse du client
         		HLitRecherchePremier(Adresse,IDClientAdresseDéfaut,[gUUIDClient,1])
         		SI HTrouve(Adresse) ALORS
         			
         			SAI_AdresseNomComplet = Adresse.NomComplet
         			SAI_AdresseSociété = Adresse.Societe
         			SAI_AdresseVoie = Adresse.Voie
         			SAI_AdresseComplément = Adresse.Complement
         			SAI_AdresseBoîtePostale = Adresse.LieuDitBP
         			SAI_AdresseCodePostal = Adresse.CodePostal
         			SAI_AdresseVille = Adresse.Ville
         			SAI_AdresseEtatProvince = Adresse.EtatProvince
         			nIndicePays est un entier = ListeCherche(COMBO_AdressePays,Adresse.Pays)
         			SI nIndicePays > 0 ALORS ListeSelectPlus(COMBO_AdressePays, nIndicePays)
         			
         			//Adresse de facturation, pré-remplie avec l'adresse du client
         			SAI_AdresseFactNomComplet = Adresse.NomComplet
         			SAI_AdresseFactSociété = Adresse.Societe
         			SAI_AdresseFactVoie = Adresse.Voie
         			SAI_AdresseFactComplément = Adresse.Complement
         			SAI_AdresseFactBoîtePostale = Adresse.LieuDitBP
         			SAI_AdresseFactCodePostal = Adresse.CodePostal
         			SAI_AdresseFactVille = Adresse.Ville
         			SAI_AdresseFactEtatProvince = Adresse.EtatProvince
         			nIndicePays = ListeCherche(COMBO_AdresseFactPays,Adresse.Pays)
         			SI nIndicePays > 0 ALORS ListeSelectPlus(COMBO_AdresseFactPays, nIndicePays)
         			
         		FIN
         		
         		INT_AdresseFacturationIdentique = 1
         		GR_AdresseFacturation..Etat = Grisé
         		
         		
         	FIN
         	
         fin
         
         
         //Gestion du statut : si c'est une création de devis, on bloque la combo du statut
         //On gère un bouton à la place pour passer du statut devis au statut commande
         //Car ce changement de statut n'est pas anecdotique
         ListeSelectPlus(COMBO_EtatCommande, gnStatut)
         si gnStatut = CDE_DEVIS alors
         	
         	BTN_Transférer_en_cde..Visible = Vrai 
         	BTN_Facturer..Visible = Faux
         	GR_Commande_Facture..visible = Faux
         	
         sinon
         	BTN_Transférer_en_cde..Visible = Faux
         	
         	//Si on est sur une commande, (et non un devis)
         	//et que la commande n'est pas encore facturée, on propose l'option de facturation
         	SI gnIDCommande <> -1  _et_ Commande.NumFacture ~= "" ALORS
         		BTN_Facturer..Visible = Vrai 
         	sinon
         		BTN_Facturer..Visible = faux 
         	FIN
         	
         	GR_Commande_Facture..Visible = vrai
         FIN
         
         
