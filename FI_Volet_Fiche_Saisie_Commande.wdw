   -
     name : SaisieAssistée
     internal_properties : CAAAAAgAAAAFwB6dZyp6ynvj4YZcAUz74XRxZC7aVZSP1xAPar/XdVFgux4W3tQUPiRZi/n5dhKxh+xtPzVie0HI9H5gxX1GwWmaiYmy+cBVADXhNZui9YLDyI2gpmvkMwqSTkEbWSCipe9S0pZQ/9D8+jit474lKMMKqkH525GBVHEQZUK1SfVl9PM=
     procedure_id : 2024323970442111286
     type_code : 14
     code : |1-
      // Résumé : Saisie assistée 
      // Syntaxe :
      //SaisieAssistée ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code, avec un différé de 30 centièmes de seconde
      // 
      
      PROCEDURE SaisieAssistée()
      
      // Si les saisies sont vides, ne rien faire
      si SAI_Ajout_Référence ~= "" _et_ SAI_Ajout_Libellé ~= "" alors
      	// Ferme la saisie assistée
      	MaFenêtreInterne..Plan = 0
      	retour
      FIN
      
      // Recherche les 10 premiers produits correspondants à la saisie
      REQ_SaisieAssistéeAjoutProduit.ParamReference = (SAI_Ajout_Référence ~= "" ? null SINON SAI_Ajout_Référence)
      REQ_SaisieAssistéeAjoutProduit.ParamLibelleDeclinaison = (SAI_Ajout_Libellé ~= "" ? Null SINON SAI_Ajout_Libellé)
      si HExécuteRequête(REQ_SaisieAssistéeAjoutProduit) = faux ALORS
      	// Ferme la saisie assistée
      	MaFenêtreInterne..Plan = 0
      	Erreur("Impossible d'exécuter la requête de saisie assistée")
      	retour
      FIN
      
      // Selon le nombre de retours
      selon HNbEnr(REQ_SaisieAssistéeAjoutProduit)
      	// Si aucune référence
      	CAS 0
      		// Ne rien faire
      		toastaffiche("Aucun produit ne correspond à la saisie", toastCourt, cvmilieu, chcentre)
      		reprisesaisie(SAI_Ajout_Référence)
      		
      	// 1 seul résultat
      	CAS 1
      		// Lit l'unique résultat
      		hlitpremier(REQ_SaisieAssistéeAjoutProduit)
      		// Mémorise les informations dans les champs de saisie
      		SAI_Ajout_Référence = REQ_SaisieAssistéeAjoutProduit.Reference
      		SAI_Ajout_libellé = REQ_SaisieAssistéeAjoutProduit.LibelleDeclinaison
      		// Ferme la saisie assistée
      		MaFenêtreInterne..plan = 0
      		// Positionne la saisie sur le bouton d'ajout
      		reprisesaisie(BTN_Ajouter)
      		
      	// Plusieurs résultats
      	autres cas
      		
      		// Vide la zone répétée
      		ZoneRépétéeSupprimeTout(ZR_DéclinaisonProduit)
      		
      		// Tableau des déclinaisons de produits, on affichera son contenu dans la ZR 
      		tabListeDéclinaisons est un tableau de cDéclinaisonProduit
      		
      		// Parcours de la requête et on stocke les déclinaisons de produits dans un tableau
      		clDeclProduit est un cDéclinaisonProduit
      		POUR TOUTE REQ_SaisieAssistéeAjoutProduit 
      			FichierVersMémoire(clDeclProduit,REQ_SaisieAssistéeAjoutProduit)
      			TableauAjoute(tabListeDéclinaisons, clDeclProduit)
      		FIN
      		
      		// Affiche la liste des déclinaisons de produit
      		POUR TOUT ELEMENT clDeclProduit DE tabListeDéclinaisons
      			ZoneRépétéeAjouteLigne(ZR_DéclinaisonProduit,clDeclProduit.m_sPhoto,clDeclProduit.m_sReference,clDeclProduit.m_sLibelleDeclinaison, clDeclProduit.m_rPrixUnitaireHT,clDeclProduit.m_bPromotion,clDeclProduit.m_bNouveau, clDeclProduit.m_nIDDéclinaisonProduit)
      		FIN
      		
      		// Affiche la saisie assistée
      		mafenetreinterne..plan = 1
      		
      Fin
