                    p_codes :
                     -
                       code : |1+
                        //Si c'est une nouvelle commande / devis >> processus de création d'un enregistrement de commande avec un Numéro généré
                        SI gnIDCommande = -1 ALORS 
                        	
                        	commandecrée()
                        	Commande.UUIDClient = gUUIDClient
                        	
                        sinon
                        	
                        	//Si c'est un devis ou une commande que l'on modifie
                        	//On supprime les adresses de livraison et de facturation si elles sont modifiées pour pouvoir les recréer
                        	
                        	nIDCommandeLivraison est un entier = commande.IDAdresseLivraison 
                        	
                        	si GR_AdresseLivraison..modifie = vrai alors
                        		
                        		commande.IDAdresseLivraison  = 0 
                        		hmodifie(commande)
                        		
                        		HLitRecherchePremier(AdresseArchive,IDAdresse, nIDCommandeLivraison)
                        		si htrouve(AdresseArchive) ALORS
                        			hsupprime(AdresseArchive)
                        		FIN
                        	fin
                        	
                        	si INT_AdresseFacturationIdentique..modifie = vrai ou GR_AdresseFacturation..modifié = vrai alors
                        		nIDCommandeFacturation est un entier = commande.IDAdresseFacturation 
                        		Commande.IDAdresseFacturation  = 0 
                        		HModifie(Commande)
                        		
                        		si nIDCommandeLivraison <> nIDCommandeFacturation alors
                        			
                        			HLitRecherchePremier(AdresseArchive,IDAdresse, Commande.IDAdresseFacturation )
                        			SI HTrouve(AdresseArchive) ALORS
                        				HSupprime(AdresseArchive)
                        			FIN
                        		fin
                        	fin
                        	
                        	//On supprime les lignes de commande si elles ont été modifiées pour pouvoir les recréer
                        	si TABLE_LigneCde..modifie = vrai alors
                        		pour TOUTE LigneCde AVEC IDCommande = commande.IDCommande
                        			hsupprime(lignecde)
                        		fin
                        	fin
                        	
                        fin
                        
                        
                        EcranVersFichier()
                        
                        
                        //Si nouvelle commande/nouveau devis ou si les lignes de commande ont été modifiées 
                        si gnIDCommande = -1 ou TABLE_LigneCde..Modifié = Vrai alors
                        	
                        	//On les crée
                        	
                        	POUR TOUTE LIGNE De table_lignecde
                        		
                        		// Recherche du produit et de son taux de taxe
                        		SI HLitRecherchePremier(DéclinaisonProduit,Reference, COL_Reference) = Faux ALORS
                        			Erreur(chaineconstruit("Référence %1 inconnue",COL_Reference))
                        			retour
                        		FIN
                        		si HLitRecherchePremier(Produit,IDProduit,DéclinaisonProduit.IDProduit) = Faux ou 
                        			HLitRecherchePremier(TauxTaxe,IDTauxTaxe,Produit.IDTauxTaxe) = Faux alors
                        			Erreur(chaineconstruit("Taux de taxe inconnu pour le produit %1", COL_Reference))
                        			RETOUR
                        		FIN
                        		
                        		// Création de la ligne de commande
                        		HRAZ(LigneCde)
                        		LigneCde.LibelleProduit = DéclinaisonProduit.LibelleDeclinaison
                        		LigneCde.Quantite = COL_Quantite
                        		LigneCde.PrixUnitaireHT = DéclinaisonProduit.PrixUnitaireHT
                        		LigneCde.TauxTaxe = TauxTaxe.Taux
                        		LigneCde.TotalLigne = LigneCde.PrixUnitaireHT * LigneCde.Quantite
                        		LigneCde.IDCommande = commande.IDCommande
                        		LigneCde.IDDéclinaisonProduit = DéclinaisonProduit.IDDéclinaisonProduit
                        		SI HAjoute(LigneCde)=Faux ALORS
                        			Erreur(HErreurInfo())
                        		FIN	
                        		
                        	FIN
                        fin
                        
                        SI gnIDCommande = -1 OU GR_AdresseLivraison..Modifié = Vrai ALORS
                        	
                        	//Adresse de livraison
                        	AdresseArchive.NomComplet = SAI_AdresseNomComplet
                        	AdresseArchive.societe = SAI_AdresseSociété
                        	AdresseArchive.Voie = SAI_AdresseVoie
                        	AdresseArchive.Complement = SAI_AdresseComplément
                        	AdresseArchive.LieuDitBP = SAI_AdresseBoîtePostale
                        	AdresseArchive.CodePostal = SAI_AdresseCodePostal
                        	AdresseArchive.Ville = SAI_AdresseVille
                        	AdresseArchive.EtatProvince = SAI_AdresseEtatProvince
                        	AdresseArchive.Pays = COMBO_AdressePays[COMBO_AdressePays]
                        	
                        	si hajoute(AdresseArchive) = vrai alors
                        		commande.IDAdresseLivraison = AdresseArchive.IDAdresse
                        	fin
                        fin
                        
                        
                        SI gnIDCommande = -1 OU INT_AdresseFacturationIdentique..Modifié = Vrai OU GR_AdresseFacturation..Modifié = Vrai ALORS
                        	
                        	si INT_AdresseFacturationIdentique = 1 ALORS
                        		Commande.IDAdresseFacturation = Commande.IDAdresseLivraison
                        	sinon
                        		
                        		AdresseArchive.NomComplet = sai_adressefactNomComplet
                        		AdresseArchive.Societe = SAI_AdresseFactSociété
                        		AdresseArchive.Voie = SAI_AdresseFactVoie
                        		AdresseArchive.Complement = SAI_AdresseFactComplément
                        		AdresseArchive.LieuDitBP = SAI_AdresseFactBoîtePostale
                        		AdresseArchive.CodePostal = SAI_AdresseFactCodePostal
                        		AdresseArchive.Ville = SAI_AdresseFactVille
                        		AdresseArchive.EtatProvince = SAI_AdresseFactEtatProvince
                        		AdresseArchive.Pays = COMBO_AdressefactPays[COMBO_AdresseFactPays]
                        		SI HAjoute(AdresseArchive) = Vrai ALORS
                        			Commande.IDAdresseFacturation = AdresseArchive.IDAdresse
                        		FIN
                        		
                        	fin
                        	
                        fin
                        
                        SI HModifie(Commande) = vrai ALORS
                        	Notifie("Ajout.Document", commande.IDCommande)	
                        	toastaffiche("Enregistré")
                        sinon
                        	Erreur(HErreurInfo())
                        FIN
                        
