  procedures :
   -
     name : AfficheCaractéristiquesProduit
     procedure_id : 1712498753519463944
     type_code : 14
     code : |1+
      // Résumé : Affiche les caractéristiques d'un produit "général"
      // Syntaxe :
      //AfficheCaractéristiquesProduit (<nIDProduit> est entier)
      //
      // Paramètres :
      //	nIDProduit (entier) : ID du produit
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE AfficheCaractéristiquesProduit(LOCAL nIDProduit est un entier)
      
      HLitRecherche(Produit,IDProduit,nIDProduit)
      
      SI HTrouve(Produit) ALORS
      	
      	zmlSupprimeTout(ZM_FiltresCaractéristiques)
      	
      	REQ_CaractéristiquesDunEnsemble.ParamIDEnsembleCaractéristique = Produit.IDEnsembleCaractéristique
      	HExécuteRequête(REQ_CaractéristiquesDunEnsemble)
      	
      	POUR TOUT REQ_CaractéristiquesDunEnsemble 
      		
      		//On crée une ligne dans la ZML, elle contiendra la fenêtre interne avec les valeurs possibles pour cette caractéristique
      		nIndice est un entier = zmlAjouteLigne(ZM_FiltresCaractéristiques)
      		
      		//On indique ici à la fenêtre interne quelle caractéristique elle gère
      		nHauteur est un entier = ZM_FiltresCaractéristiques[nIndice].CFI_Filtre.AjouterValeursCaractéristique(REQ_CaractéristiquesDunEnsemble.IDCaractéristique, REQ_CaractéristiquesDunEnsemble.TypeCaractéristique)
      		//On adapte la hauteur de la ligne de ZML pour afficher toutes les options
      		ZM_FiltresCaractéristiques[nIndice]..Hauteur =  nHauteur + 20
      		ZM_FiltresCaractéristiques[nIndice].CFI_Filtre..Hauteur =  nHauteur + 20
      		
      		//Cocher la valeur de caractéristique correspondant au produit décliné
      		REQ_CaractéristiquesDuneDéclinaison.ParamIDDéclinaisonProduit = gId
      		REQ_CaractéristiquesDuneDéclinaison.ParamIDCaractéristique = REQ_CaractéristiquesDunEnsemble.IDCaractéristique
      		HExécuteRequête(REQ_CaractéristiquesDuneDéclinaison)
      		HLitPremier(REQ_CaractéristiquesDuneDéclinaison)
      		ZM_FiltresCaractéristiques[nIndice].CFI_Filtre.CocherValeurCaractéristique(REQ_CaractéristiquesDuneDéclinaison.IDValeurCaractéristique)
      		
      	fin	
      FIN
