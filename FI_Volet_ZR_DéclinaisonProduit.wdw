  procedures :
   -
     name : MAJListeProduits
     procedure_id : 1630981210617996037
     type_code : 14
     code : |1+
      // Résumé : Met à jour la liste des produits dans la fenêtre, en fonction des filtres choisis
      // Syntaxe :
      //MAJListeProduits ( [<nMode> est entier])
      //
      // Paramètres :
      //	nMode (entier - valeur par défaut=1) :Tout afficher ou filtrer l'affichage des produits
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MAJListeProduits(nMode est un entier = CST_TOUTAFFICHER)
      
      //Tableau des déclinaisons de produits, on affichera son contenu dans la ZR 
      tabListeDéclinaisons est un tableau de cDéclinaisonProduit
      
      //On vide la ZR
      ZoneRépétéeSupprimeTout(ZR_DéclinaisonProduit)
      
      
      SAI_Libellé = ""
      SAI_Référence = ""
      
      //Paramétrage de la requête en fonction de la famille de produit choisie par l'utilisateur
      si nMode = CST_TOUTAFFICHER ALORS
      	
      	//Ici, on sait que l'on doit tout afficher, .. pas de filtre ni sur les familles, ni sur les caractéristiques
      	REQ_DéclinaisonsProduit.ParamIDFamilleProduit = null
      	
      sinon
      	
      	//Recherche les déclinaisons de produits de cette famille et des sous-familles de la famille 
      	tabFamille est un tableau d'entiers = FamilleDonneSousFamilles(TABLEH_Familles.COL_ID)
      	REQ_DéclinaisonsProduit.ParamIDFamilleProduit = TableauVersChaîne(tabFamille, TAB)
      	
      FIN
      
      //Exécution de la requête
      HExécuteRequête(REQ_DéclinaisonsProduit)
      
      //Parcours de la requête et on stocke les déclinaisons de produits dans un tableau
      clDeclProduit est un cDéclinaisonProduit
      pour TOUTE REQ_DéclinaisonsProduit 
      	FichierVersMémoire(clDeclProduit,REQ_DéclinaisonsProduit)
      	TableauAjoute(tabListeDéclinaisons, clDeclProduit)
      FIN
      
      //On va épurer le tableau selon les filtres posés par l'utilisateur sur les caractéristiques
      FiltrerDéclProdSelonCaractéristiques(tabListeDéclinaisons)
      
      //On peut afficher la liste des déclinaisons de produit
      pour tout element clDeclProduit de tabListeDéclinaisons
      	ZoneRépétéeAjouteLigne(ZR_DéclinaisonProduit,clDeclProduit.m_sPhoto,clDeclProduit.m_sReference,clDeclProduit.m_sLibelleDeclinaison, clDeclProduit.m_rPrixUnitaireHT,clDeclProduit.m_bPromotion,clDeclProduit.m_bNouveau, clDeclProduit.m_nIDDéclinaisonProduit)
      FIN
      
      
      //Affichage du nombre de produits 
      lib_NbProduits = ChaîneConstruit( "  Total : %1 produit(s)", tabListeDéclinaisons..occurrence)
     type : 458752
   -
     name : AfficherFiltresCaractéristiques
     procedure_id : 1632056915438829437
     type_code : 14
     code : |1-
      // Résumé : Affichage des filtres
      // Syntaxe :
      // AfficherFiltresCaractéristiques ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficherFiltresCaractéristiques()
      
      //Pour chaque caractéristique
      POUR TOUTE Caractéristique 
      	
      	//On crée une ligne dans la ZML, elle contiendra la fenêtre interne avec les valeurs possibles pour cette caractéristique
      	nIndice est un entier = zmlAjouteLigne(ZM_FiltresCaractéristiques)
      	
      	//On indique ici à la fenêtre interne quelle caractéristique elle gère
      	nHauteur est un entier = ZM_FiltresCaractéristiques[nIndice].CFI_Filtre.AjouterValeursCaractéristique(Caractéristique.IDCaractéristique, Caractéristique.TypeCaractéristique)
      	//On adapte la hauteur de la ligne de ZML pour afficher toutes les options
      	ZM_FiltresCaractéristiques[nIndice]..Hauteur =  nHauteur + 20
      	ZM_FiltresCaractéristiques[nIndice].CFI_Filtre..Hauteur =  nHauteur + 20
      	
      FIN
     type : 458752
   -
     name : FiltrerDéclProdSelonCaractéristiques
     procedure_id : 1632061115917036086
     type_code : 14
     code : |1+
      // Résumé : 2pure la liste des déclinaisons de produit selon les filtres sélectionnés par l'utilisateur
      // Syntaxe :
      //FiltrerDéclProdSelonCaractéristiques (<tabListeDéclinaisons> est tableau)
      //
      // Paramètres :
      //	tabListeDéclinaisons (tableau) : Tableau des déclinaisons de produit, on le modifie directement
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE FiltrerDéclProdSelonCaractéristiques(tabListeDéclinaisons est un tableau de cDéclinaisonProduit)
      
      clDeclProduit est un cDéclinaisonProduit
      tabValeurCarat est un tableau d'entiers
      
      //Pour chaque ligne de la ZML, donc pour chaque caractéristique, 
      POUR i = 1 _a_ ZM_FiltresCaractéristiques..Occurrence
      	
      	// on demande à la fenêtre interne qui affiche les valeurs de cette caractéristique
      	//de nous dire quelles valeurs sont sélectionnées par l'utilisateur 
      	tabValeurCarat  = ZM_FiltresCaractéristiques[i].CFI_Filtre.DonnerValeursSélectionnées()
      	
      	//S'il n'y a pas de filtre posé sur cette caractéristique, on n'a pas besoin d'épurer le tableau 
      	si tabValeurCarat..occurrence > 0 alors 
      		
      		//et s'il y a un filtre sur cette caractéristique, on parcourt le tableau des déclinaisons de produits qu'on voulait afficher
      		//et on supprime les déclinaisons qui ne répondent pas aux critères
      		pour j = tabListeDéclinaisons..occurrence a 1 pas -1
      			
      			clDeclProduit = tabListeDéclinaisons[j]
      			
      			//requête qui récupère pour une déclinaison de produit donnée, et une caractéristique donnée, la valeur de cette caractéristique
      			REQ_CaractéristiquesDuneDéclinaison.ParamIDDéclinaisonProduit = clDeclProduit.m_nIDDéclinaisonProduit
      			REQ_CaractéristiquesDuneDéclinaison.ParamIDCaractéristique = ZM_FiltresCaractéristiques[i].CFI_Filtre.DonnerIDCaractéristique()
      			HExécuteRequête(REQ_CaractéristiquesDuneDéclinaison)
      			hlitpremier(REQ_CaractéristiquesDuneDéclinaison)
      			//Si la valeur de caractéristique est parmi celles choisies par l'utilisateur, on garde la déclinaison, sinon on supprime
      			si tableaucherche(tabValeurCarat,tcLinéaire, REQ_CaractéristiquesDuneDéclinaison.IDValeurCaractéristique) < 0 alors
      				TableauSupprime(tabListeDéclinaisons,j)
      			FIN
      			
      		FIN
      	fin
      	
      FIN
      
     type : 458752
