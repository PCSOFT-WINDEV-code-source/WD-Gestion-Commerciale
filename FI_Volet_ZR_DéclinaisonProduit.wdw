      -
        code : |1+
         PROCEDURE MaFenêtre(gnMode est un entier = CST_TOUTAFFICHER, gstFiltresRecherche est un STFiltresRechercheProduits = Null)
         
      -
        code : |1+
         DeclareProcedureNotification("Modif.DéclinaisonProduit",SurModification)
         
         	PROCEDURE INTERNE SurModification(sParametre <utile>)
         	MAJListeProduits(CST_TOUTAFFICHER)
         	FIN
         
         DeclareProcedureNotification("Ajout.DéclinaisonProduit",SurAjout)
         
         	PROCEDURE INTERNE SurAjout(sParametre <utile>)
         	MAJListeProduits(CST_TOUTAFFICHER)
         	FIN
         
         DeclareProcedureNotification("Suppression.DéclinaisonProduit",SurSuppression)
         
         	PROCEDURE INTERNE SurSuppression(sParametre <utile>)
         	ZoneRépétéeSupprime(ZR_DéclinaisonProduit)
         	FIN
         
         
         
         //L'arbre des familles est déroulé
         TableDérouleTout(TABLEH_Familles)
         
         //On affiche les filtres de caractéristiques
         AfficherFiltresCaractéristiques()
         
         
         SI gnMode = CST_FILTRE ALORS
         	
         	SAI_Référence = gstFiltresRecherche.sRéférence
         	SAI_Libellé = gstFiltresRecherche.sLibellé
         	ExécuteTraitement(BTN_Recherche, trtclic)	
         	
         sinon
         	
         	//Par défaut, tous les produits sont affichés
         	MAJListeProduits(gnMode)
         	
         FIN
